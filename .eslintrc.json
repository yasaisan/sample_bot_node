{
    "env": {
        "node": true
    },
    "plugins": ["node"],
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018
    },
    "rules": {
        "node/no-unsupported-features": ["error", {"version": 8}]
    }
}
